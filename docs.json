{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Speedy Services Blog",
  "colors": {
    "primary": "#e36f14",
    "light": "#f68800",
    "dark": "#b0560f"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "versions": [
      {
        "version": "1.0.0",
        "default": true,
        "href": "https://blog.speedyservices.org"
      }
    ]
  },
  "appearance": {
    "default": "light",
    "strict": true
  },
  "search": { "prompt": "Search for articles" },
  "navbar": {
    "primary": {
      "type": "button",
      "label": "Checkout our website",
      "href": "https://speedyservices.org"
    }
  },
  "logo": {
    "light": "/speedy-services-logo.svg",
    "dark": "/speedy-services-logo.svg"
  },
  "footer": {
    "socials": {
      "instagram": "https://www.instagram.com/speedy_services.lb",
      "linkedin": "https://linkedin.com/company/speedy-services"
    }
  }
}
